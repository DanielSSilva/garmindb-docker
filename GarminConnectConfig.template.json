{
    "db": {
        "type": "sqlite"
    },
    "garmin": {
        "domain": "garmin.com"
    },
    "credentials": {
        "user": "${APP_USER}",
        "secure_password": false,
        "password": "${APP_PASSWORD}"
    },
    "data": {
        "weight_start_date": "12/31/2023",
        "sleep_start_date": "12/31/2023",
        "rhr_start_date": "12/31/2023",
        "monitoring_start_date": "12/31/2023",
        "download_latest_activities": 25,
        "download_all_activities": 1000
    },
    "directories": {
        "relative_to_home": true,
        "base_dir": "HealthData",
        "mount_dir": "/Volumes/GARMIN"
    },
    "enabled_stats": {
        "monitoring": true,
        "steps": true,
        "itime": true,
        "sleep": true,
        "rhr": true,
        "weight": true,
        "activities": true
    },
    "course_views": {
        "steps": []
    },
    "modes": {},
    "activities": {
        "display": []
    },
    "settings": {
        "metric": false,
        "default_display_activities": ["walking", "running", "cardio"]
    },
    "checkup": {
        "look_back_days": 90
    }
}